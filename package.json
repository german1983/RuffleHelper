{
  "name": "ruffle-helper",
  "version": "0.0.1",
  "description": "Automatic run ruffles in a smart way.",
  "engines": {
    "node": "12.x"
  },
  "main": "index.js",
  "scripts": {
    "serve": "node ./index.js",
    "app-dev": "npm run clean && npm run watch",
    "app-build": "npm run clean && npm run heroku-postbuild",
    "watch": "webpack --config webpack.dev.js --watch",
    "heroku-postbuild": "webpack --config webpack.prod.js -p",
    "clean": "rm -rf ./dist",
    "test": "jest --collectCoverage",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "test:updateSnapshot": "./node_modules/.bin/jest --updateSnapshot"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.2",
    "@types/jest": "^24.0.22",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "ejs": "^2.5.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "express": "^4.17.3",
    "react": "^16.11.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "ts-jest": "^24.1.0",
    "typesafe-actions": "^5.1.0-1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.1",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.5",
    "@types/redux-mock-store": "^1.0.1",
    "axios": "^0.19.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-root-plugin": "^0.10.0",
    "node-sass": "^4.13.1",
    "react-scripts": "^3.3.0",
    "redux-mock-store": "^1.5.3",
    "request": "^2.81.0",
    "tape": "^4.7.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.7.3",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/german1983/RuffleHelper"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "license": "MIT"
}
